{
  "name": "horizon-chat",
  "version": "1.0.0",
  "description": "Horizon Chat app example ported to Elm",
  "main": "index.js",
  "scripts": {
    "build": "elm make src/Chat.elm --output dist/app.js",
    "watch": "watch \"elm make src/Chat.elm --output dist/app.js\" src",
